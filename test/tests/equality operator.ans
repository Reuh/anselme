:a::constant = [1:2]

|false = {a == [5:2]}

|false = {a == [1:3]}

|true = {a == [1:2]}

:b::constant = [1,2,3]

|false = {b == a}

|false = {b == []}

|false = {b == [3,1,2]}

|false = {b == [1,2,3,4]}

|true = {b == [1,2,3]}

:c = *[1,2,3]

|false = {c == b}

|true = {c!to tuple == b}

:d::constant = [1,2,3]

|true = {d == b}
